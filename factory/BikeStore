package factory;

public class BikeStore(){

    public Bike createBike(String type){

        if(type.equals("tricycle")){
            type = new Tricycle();
        }
        else if(type.equals("strider")){
            type = new Strider();
        }
        else if(type.equals("kids bike")){
            type = new KidsBike();
        }
        else(
            bike = null;
        )

        return bike;
    }

}